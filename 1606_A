#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    
    while (t-- > 0) {
        string s;
        cin >> s;
        int len = s.length();
        
        if(len == 1) cout << s << endl;
        else if(s[0] == s[len - 1]) cout << s << endl; 
        else {
            cout << s.substr(0, len - 1) << s[0] << endl;
        }
    }
    return 0;
}
