#Moca & Math

#include <bits/stdc++.h>

using namespace std;

int main() {
    int t;
    cin >> t;

    while (t--) {
        int n;
        cin >> n;
        vector<int> vec(n + 1);
        
        for(int i=1;i<=n;i++)cin>>vec[i];
    int res=vec[1];
    for(int i=2;i<=n;i++)res&=vec[i];
    cout<<res<<endl;
    }

    return 0;
}
