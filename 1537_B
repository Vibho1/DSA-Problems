#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    int t;
    cin >> t;

    while (t--) {
        int n, m, i, j;
        cin >> n >> m >> i >> j;
        
        cout << 1 << " " << 1 << " " << n << " " << m;
        
        cout << endl;
    }

    return 0;
}
